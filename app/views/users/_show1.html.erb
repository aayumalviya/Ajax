
    <tr id=<%= "user-#{user.id}"%>>
        <td><%= user.name %></td>
        <td><%= user.content %></td>
        <td><%= link_to "Show", user_path(user) %></td>
        <td><%= link_to "Edit" , edit_user_path(user) %></td>
        <td><%= link_to "Destroy", user_path(user), remote:true,  method: :delete  %></td>
    </tr>
